https://practice.geeksforgeeks.org/problems/prime-number2314/1?page=1&difficulty[]=-1&sortBy=submissions




class Solution{
    static int isPrime(int N){
        // code here
        if(N==1 || N<0)
        {
            return 0;
        }
        if(N==2)
        {
            return 1;
        }
        for(int i=2;i<=N/2;i++)
        {
            if(N%i==0)
            {
                return 0;
            }
            
        }
        return 1;
    
        
    }
}
